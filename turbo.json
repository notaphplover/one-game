{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["src/**/*.ts"],
      "outputs": ["dist/**", "lib/**"]
    },
    "@cornie-js/api-json-schemas#build": {
      "dependsOn": ["^build"],
      "inputs": ["schemas/**/*.json"]
    },
    "@cornie-js/api-openapi-schema#build": {
      "dependsOn": ["^build"],
      "inputs": ["schemas/src/one-game.yaml", "src/**/*.ts"],
      "outputs": ["dist/**", "lib/**", "schemas/generated/one-game.yaml"]
    },
    "@cornie-js/api-models#build": {
      "dependsOn": ["^build"],
      "inputs": ["src/**/*.ts"],
      "outputs": ["lib/**", "src/models/types.ts"]
    },
    "@cornie-js/backend-app-game#build": {
      "dependsOn": ["^build", "@cornie-js/backend-app-game-fixtures#build"],
      "inputs": ["src/**/*.ts"],
      "outputs": ["lib/**"]
    },
    "@cornie-js/backend-app-game-db#build": {
      "dependsOn": ["^build", "@cornie-js/backend-app-game-fixtures#build"],
      "inputs": ["src/**/*.ts"],
      "outputs": ["lib/**"]
    },
    "@cornie-js/backend-app-user#build": {
      "dependsOn": ["^build", "@cornie-js/backend-app-user-fixtures#build"],
      "inputs": ["src/**/*.ts"],
      "outputs": ["lib/**"]
    },
    "@cornie-js/backend-app-user-db#build": {
      "dependsOn": ["^build", "@cornie-js/backend-app-user-fixtures#build"],
      "inputs": ["src/**/*.ts"],
      "outputs": ["lib/**"]
    },
    "@cornie-js/web-ui#build": {
      "dependsOn": ["^build"],
      "inputs": ["src/**/*.{cjs,js,jsx}", "vite.config.js"]
    },
    "close:e2e": {
      "cache": false
    },
    "format": {
      "inputs": ["src/**/*.ts"],
      "outputs": ["src/**/*.ts"]
    },
    "lint": {
      "dependsOn": ["^build"],
      "inputs": ["src/**/*.ts"]
    },
    "migrations:run": {
      "dependsOn": ["build"],
      "inputs": ["dist/**", "lib/**"]
    },
    "serve": {
      "cache": false,
      "dependsOn": ["build"],
      "persistent": true
    },
    "start:e2e": {
      "cache": false,
      "dependsOn": ["build"]
    },
    "test": {
      "inputs": ["src/**/*.ts"],
      "outputs": []
    },
    "test:integration:js": {
      "dependsOn": ["build"],
      "inputs": ["dist/**", "lib/**"],
      "outputs": []
    },
    "test:js": {
      "dependsOn": ["build"],
      "inputs": ["dist/**", "lib/**"],
      "outputs": []
    },
    "test:js:coverage": {
      "dependsOn": ["build"],
      "inputs": ["dist/**", "lib/**"],
      "outputs": ["coverage/**"]
    },
    "test:uncommitted": {
      "inputs": ["src/**/*.ts"],
      "outputs": []
    },
    "test:unit:js": {
      "dependsOn": ["build"],
      "inputs": ["dist/**", "lib/**"],
      "outputs": []
    }
  }
}
