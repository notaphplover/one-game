{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "src/**/*.ts"
      ],
      "outputs": [
        "dist/**",
        "lib/**"
      ]
    },
    "backend-api-models#build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "src/**/*.ts",
        "schemas/**/*.json"
      ],
      "outputs": [
        "lib/**"
      ]
    },
    "format": {
      "inputs": [
        "src/**/*.ts"
      ],
      "outputs": [
        "src/**/*.ts"
      ]
    },
    "lint": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "src/**/*.ts"
      ]
    },
    "test": {
      "inputs": [
        "src/**/*.ts"
      ],
      "outputs": []
    },
    "test:integration:js": {
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "dist/**",
        "lib/**"
      ],
      "outputs": []
    },
    "test:js": {
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "dist/**",
        "lib/**"
      ],
      "outputs": []
    },
    "test:js:coverage": {
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "dist/**",
        "lib/**"
      ],
      "outputs": [
        "coverage/**"
      ]
    },
    "test:uncommitted": {
      "inputs": [
        "src/**/*.ts"
      ],
      "outputs": []
    },
    "test:unit:js": {
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "dist/**",
        "lib/**"
      ],
      "outputs": []
    }
  }
}
