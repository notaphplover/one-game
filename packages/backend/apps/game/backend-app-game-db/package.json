    "@cornie-js/backend-game-domain": "workspace:*",
    "@cornie-js/backend-app-game-models": "workspace:*",
    "@cornie-js/backend-common": "workspace:*",
    "@cornie-js/backend-db": "workspace:*",
    "pg": "8.11.0",
