    "app-user-revert-migration": "./scripts/revertMigration.mjs",
    "app-user-run-migration": "./scripts/runMigrations.mjs",
    "app-user-seed-database": "./scripts/seedDatabase.mjs"
  },
  "bugs": {
    "url": "https://github.com/notaphplover/one-game/issues"
  },
  "description": "Backend user app",
  "dependencies": {
    "@nestjs/common": "10.0.5",
