{
  "name": "@cornie-js/web-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm run typecheck && vite build",
    "format": "prettier --write ./src/**/*.{css,js,jsx,scss}",
    "lint": "eslint --ext ts --ext tsx --ignore-path .gitignore ./src",
    "preview": "vite preview",
    "test": "dotenv -e .test.env -- jest --config=jest.config.mjs --runInBand --verbose",
    "test:integration:js": "pnpm run test --selectProjects Integration",
    "test:js": "pnpm run test",
    "test:js:coverage": "pnpm run test:js --coverage",
    "test:uncommitted": "pnpm run test --changedSince=HEAD",
    "test:unit:js": "pnpm run test --selectProjects Unit",
    "test:watch": "pnpm run test --watchAll",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@cornie-js/api-http-client": "workspace:*",
    "@cornie-js/api-models": "workspace:*",
    "@cornie-js/frontend-api-rtk-query": "workspace:*",
    "@emotion/react": "11.13.0",
    "@emotion/styled": "11.13.0",
    "@mui/icons-material": "5.16.5",
    "@mui/material": "5.16.5",
    "@reduxjs/toolkit": "2.2.7",
    "async-mutex": "0.5.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-redux": "9.1.2",
    "react-router-dom": "6.25.1"
  },
  "devDependencies": {
    "@babel/cli": "7.24.8",
    "@babel/core": "7.25.2",
    "@babel/preset-env": "7.25.2",
    "@babel/preset-react": "7.24.7",
    "@babel/preset-typescript": "7.24.7",
    "@jest/globals": "29.7.0",
    "@jest/types": "29.6.3",
    "@testing-library/react": "^16.0.0",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "@vitejs/plugin-react": "4.3.1",
    "babel-jest": "29.7.0",
    "babel-preset-vite": "1.1.3",
    "dotenv-cli": "7.4.2",
    "eslint": "8.57.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "28.6.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react-hooks": "4.6.2",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "prettier": "3.3.3",
    "sass": "1.77.8",
    "typescript": "5.5.4",
    "vite": "5.3.5"
  }
}
