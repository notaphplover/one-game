{
  "name": "@cornie-js/web-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm run typecheck && vite build",
    "format": "prettier --write ./src/**/*.{css,js,jsx,scss}",
    "preview": "vite preview",
    "test": "dotenv -e .test.env -- jest",
    "test:js": "pnpm run test",
    "test:uncommitted": "pnpm run test --changedSince=HEAD",
    "test:unit:js": "pnpm run test:js",
    "test:watch": "pnpm run test --watchAll",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@cornie-js/api-http-client": "workspace:*",
    "@cornie-js/api-models": "workspace:*",
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "5.15.11",
    "@mui/material": "5.15.11",
    "@reduxjs/toolkit": "2.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "9.1.0",
    "react-router-dom": "6.22.2"
  },
  "devDependencies": {
    "@babel/cli": "7.23.9",
    "@babel/core": "7.24.0",
    "@babel/preset-env": "7.24.0",
    "@babel/preset-react": "7.23.3",
    "@babel/preset-typescript": "7.23.3",
    "@jest/globals": "29.7.0",
    "@jest/types": "29.6.3",
    "@testing-library/react": "^14.0.0",
    "@types/react": "18.2.61",
    "@types/react-dom": "18.2.19",
    "@vitejs/plugin-react": "4.2.1",
    "babel-jest": "29.7.0",
    "babel-preset-vite": "1.1.3",
    "dotenv-cli": "7.3.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "prettier": "3.2.5",
    "sass": "1.71.1",
    "typescript": "5.3.3",
    "vite": "5.1.4"
  }
}
