{
  "name": "@cornie-js/web-ui",
  "private": true,
  "version": "0.0.4",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm run typecheck && vite build",
    "format": "prettier --write ./src/**/*.{css,js,jsx,scss}",
    "lint": "eslint ./src",
    "preview": "vite preview",
    "test": "dotenv -e .test.env -- jest --config=jest.config.mjs --runInBand --verbose",
    "test:integration:js": "pnpm run test --selectProjects Integration",
    "test:js": "pnpm run test",
    "test:js:coverage": "pnpm run test:js --coverage",
    "test:uncommitted": "pnpm run test --changedSince=HEAD",
    "test:unit:js": "pnpm run test --selectProjects Unit",
    "test:watch": "pnpm run test --watchAll",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@cornie-js/api-http-client": "workspace:*",
    "@cornie-js/api-models": "workspace:*",
    "@cornie-js/eventsource": "workspace:*",
    "@cornie-js/frontend-api-rtk-query": "workspace:*",
    "@cornie-js/frontend-common": "workspace:*",
    "@emotion/react": "11.13.5",
    "@emotion/styled": "11.13.5",
    "@mui/icons-material": "6.1.8",
    "@mui/material": "6.1.8",
    "@reduxjs/toolkit": "2.3.0",
    "async-mutex": "0.5.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-redux": "9.1.2",
    "react-router": "7.0.1",
    "worker-timers": "8.0.11"
  },
  "devDependencies": {
    "@babel/cli": "7.25.9",
    "@babel/core": "7.26.0",
    "@babel/preset-env": "7.26.0",
    "@babel/preset-react": "7.25.9",
    "@babel/preset-typescript": "7.26.0",
    "@eslint/js": "9.15.0",
    "@jest/globals": "29.7.0",
    "@jest/types": "29.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "8.16.0",
    "@typescript-eslint/parser": "8.16.0",
    "@vitejs/plugin-react": "4.3.4",
    "babel-jest": "29.7.0",
    "babel-preset-vite": "1.1.3",
    "dotenv-cli": "7.4.4",
    "eslint": "9.15.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "prettier": "3.4.1",
    "sass": "1.81.0",
    "typescript": "5.7.2",
    "vite": "5.4.11"
  }
}
