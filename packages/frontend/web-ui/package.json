{
  "name": "@cornie-js/web-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm run typecheck && vite build",
    "format": "prettier --write ./src/**/*.{css,js,jsx,scss}",
    "preview": "vite preview",
    "test": "dotenv -e .test.env -- jest",
    "test:js": "pnpm run test",
    "test:uncommitted": "pnpm run test --changedSince=HEAD",
    "test:unit:js": "pnpm run test:js",
    "test:watch": "pnpm run test --watchAll",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@cornie-js/api-http-client": "workspace:*",
    "@cornie-js/api-models": "workspace:*",
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "@mui/icons-material": "5.15.15",
    "@mui/material": "5.15.15",
    "@reduxjs/toolkit": "2.2.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-redux": "9.1.1",
    "react-router-dom": "6.23.0"
  },
  "devDependencies": {
    "@babel/cli": "7.24.5",
    "@babel/core": "7.24.5",
    "@babel/preset-env": "7.24.5",
    "@babel/preset-react": "7.24.1",
    "@babel/preset-typescript": "7.24.1",
    "@jest/globals": "29.7.0",
    "@jest/types": "29.6.3",
    "@testing-library/react": "^15.0.0",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "4.2.1",
    "babel-jest": "29.7.0",
    "babel-preset-vite": "1.1.3",
    "dotenv-cli": "7.4.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "prettier": "3.2.5",
    "sass": "1.76.0",
    "typescript": "5.4.5",
    "vite": "5.2.10"
  }
}
